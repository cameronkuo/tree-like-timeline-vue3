{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"outDir": "dist",
		"declaration": true,
		"emitDeclarationOnly": true,
		"declarationMap": true,
		"sourceMap": true
	},
	"references": [
		{
			"path": "./tsconfig.node.json"
		}
	]
}
